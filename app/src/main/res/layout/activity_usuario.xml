<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activies.UsuarioActivity"
    tools:layout_editor_absoluteY="81dp">

    <de.hdodenhof.circleimageview.CircleImageView xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/passageiros"
        android:layout_width="356dp"
        android:layout_height="188dp"
        android:clickable="true"
        android:onClick="passageiro"
        android:src="@drawable/common_google_signin_btn_text_light_normal_background"
        app:civ_border_color="#FF000000"
        app:civ_border_width="2dp"
        tools:ignore="MissingConstraints,OnClick"
        tools:layout_editor_absoluteX="27dp"
        tools:layout_editor_absoluteY="40dp" />

    <TextView
        android:id="@+id/textNomeU"
        android:layout_width="42dp"
        android:layout_height="19dp"
        android:text="Nome:"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="261dp" />

    <TextView
        android:id="@+id/textEmailU"
        android:layout_width="38dp"
        android:layout_height="19dp"
        android:text="Email:"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="367dp" />

    <TextView
        android:id="@+id/textCpfU"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:text="CPF:"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="314dp" />

    <TextView
        android:id="@+id/textApelidoU"
        android:layout_width="51dp"
        android:layout_height="19dp"
        android:text="Apelido:"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="420dp" />

    <TextView
        android:id="@+id/texPlacaU"
        android:layout_width="38dp"
        android:layout_height="19dp"
        android:text="Placa:"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="473dp" />

    <TextView
        android:id="@+id/textTellU"
        android:layout_width="59dp"
        android:layout_height="19dp"
        android:text="Telefone:"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="525dp" />

</android.support.constraint.ConstraintLayout>